{
    "problemMatcher": [
        {
            "owner": "match-syntax",
            "pattern": [
                {
                    "regexp": "^File: (.*)",
                    "file": 1
                },
                {
                    "regexp": "^(.*), line: (\\d+).*col: (\\d+)\\)$",
                    "line": 2,
                    "column": 3,
                    "message": 1,
                    "loop": false
                }
            ]
        }
    ]
}
